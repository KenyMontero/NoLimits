                        <div class="row cuadro">
                            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                <div class="icono">
                                    <i class="fas fa-archive fa-2x text-white alinear"></i>
                                </div>
                            </div>
                            <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                <h6 class="etiqueta">Archive</h6>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <a href="@Url.Action("Build","NoLimits")">
                            <div class="row cuadro">
                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
                                    <div class="icono">
                                        <i class="fas fa-home fa-2x text-white alinear"></i>
                                    </div>
                                </div>
                                <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
                                    <h6 class="etiqueta">Build <span class="badge badge-danger">New</span></h6>
                                </div>
                            </div>
                        </a>
                    </div>
                </div>
            </div>
        </div>
