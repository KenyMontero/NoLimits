
@{
    ViewBag.Title = "NoLimit";
    Layout = "~/Views/Shared/_LayoutLimits.cshtml";
}

    <div class="content">
        <div class="row">
            <div class="col-lg-7 col-md-7 col-sm-7 col-xs-7 w-25 content2" style="height: 369px;">
